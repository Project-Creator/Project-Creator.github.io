    $(document).ready(function() {
        var Idisnametest = localStorage.getItem("testIdis");
        alert(Idisnametest);
        var elementId = document.getElementById("IdisIdentifier").getAttribute("name");
        $("#"+elementId).mousemove(function(event){            
            var relativeX = event.pageX - $(this).offset().left;
            var relativeY = event.pageY - $(this).offset().top;
            var relativeBoxCoords = "(" + relativeX + "," + relativeY + ")";
            alert(relativeBoxCoords);
        });
    }); 
